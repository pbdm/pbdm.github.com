{
  "name": "pbdm.cc",
  "version": "8.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/pbdm/pbdm.github.com.git"
  },
  "scripts": {
    "upload": "npm run push:posts && npm run push:playground && git add . && git commit -m 'update sources' && git push",
    "push:posts": "cd posts && git push",
    "push:playground": "cd playground && git push",
    "http": "http-server -c-1 -p 22222",
    "https": "http-server -c-1 -S -C server.crt -K server.pem",
    "start": "node dev.js"
  },
  "devDependencies": {
    "browser-sync": "^2.26.3",
    "http-server": "^0.11.1"
  },
  "dependencies": {}
}
